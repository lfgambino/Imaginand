@charset "utf-8";

/* Variables */
color1 = #464B51
color2 = #FF0066

body
  background-attachment: scroll
  background-color: #E5E5E5
  background-image: url("data:image/jpeg;base64,")
  background-repeat: repeat
  color: #464B51
  font-family: 'News Cycle', Verdana, Geneva, sans-serif
  font-size: 16px
  height: 100%
  margin: 0
  padding: 0
  text-align: center



footer
  color: #464B51
  font-family: Oswald, sans-serif
  font-weight: 300
  margin: 5% auto 2%
  max-width: 900px
  text-align: center
  -webkit-text-stroke: .1px

footer p
  margin-top: 0

footer #contact
  background-attachment: scroll
  background-image: url('../img/footer_bg.jpg')
  background-position: center top
  background-repeat: no-repeat
  max-height: 240px
  width: 100%

footer #contact h2
  font-family: Cookie
  font-size: 2.7em
  font-weight: 400
  margin: 0
  padding: 3% 0 1%

footer #contact h2 a
  color: rgba(255, 255, 255, .6)
  text-decoration: none

footer #contact h2 a:hover
  text-decoration: underline

footer #contact .social a
  display: inline-block
  vertical-align: middle
  margin-bottom: 10%
  margin-right: 3%

footer #contact .email
  background: url('../img/contact_sprite.png') no-repeat
  background-position 0px 0px
  height: 73px
  width: 72px
footer #contact .email:hover
  background-position -72px 0px

footer #contact .linkedin
  background: url('../img/contact_sprite.png') no-repeat
  background-position 0px -73px
  height: 65px
  width: 64px
footer #contact .linkedin:hover
  background-position -64px -73px

footer #contact .twitter
  background: url('../img/contact_sprite.png') no-repeat
  background-position 0px -138px
  height: 65px
  width: 64px
footer #contact .twitter:hover
  background-position -64px -138px



header
  color: #464B51
  overflow: hidden
  margin-bottom 4%

header h1
  font-family: 'News Cycle', Verdana, Geneva, sans-serif
  font-size: 4.2em
  font-weight: 400
  text-align: center
  padding-bottom: .2em
  border-bottom: solid 1px transparent
  margin: 0

header h1 a
  color: #464B51
  text-decoration: none

header h1:hover
  border-bottom: solid 1px #DBDBDB

header h2
  color: #7A818A
  font-size: 1.8em
  font-weight: 400
  letter-spacing: 5px
  margin: 0
  text-transform: uppercase
  -webkit-text-stroke: 0.4px #999
  text-align: center



html
  height: 100%


nav
  background rgb(101,104,109)
  left: 0
  position: fixed
  opacity .9
  top: 0
  width 100%
  z-index 1000

nav ul
  list-style-type none
  margin .5em 0
  padding 0
  text-align center
  width 100%

nav ul > li
  color #fff
  display inline-block
  font-family Oswald
  font-weight 400
  font-size 1em
  margin 0 1em
  padding 0
  text-transform uppercase
  border-bottom 1px solid rgba(255,255,255, 0)
nav ul > li:hover
  border-bottom 1px solid rgba(255,255,255, 1)

nav ul > li a
  color #fff
  text-decoration none





#aboutme
  box-sizing border-box
  padding 0 2%

#aboutme .info, #aboutme .photos, #aboutme .skills
  box-sizing border-box
  -webtik-box-sizing border-box
  -moz-box-sizing border-box
  display inline-block
  vertical-align middle

#aboutme .info
  width: 43%
  margin-left: 6%

#aboutme .info h2
  font-family: Cookie, serif
  font-size: 3.8em
  font-weight: 400
  height: 60px
  line-height: 60px
  margin: 0 0 .5em

#aboutme .info p
  font-size 1.1em
  margin: 0 0 1.5em
  text-shadow: 1px 1px #f9f9f9
  -webkit-text-stroke: 0.3px #999

#aboutme .info .contactinfo
  color: #FF0066
  -webkit-text-stroke: 0.3px #ff76ad

#aboutme .photos
  width 32%

#aboutme .photos .shadow
  background #fff
  border 1px solid rgba(89,96,102, .3)
  box-sizing border-box
  -webtik-box-sizing border-box
  -moz-box-sizing border-box
  box-shadow .1em .1em .2em .1em rgba(89,96,102, .2)
  height auto
  position relative
  width 100%
#aboutme .photos .shadow::before
  display: block
  content: ''
  position: absolute
  height 94%
  box-shadow: 0 0 .2em .1em rgba(89,96,102, .3) inset
  margin 2%
  width 96%

#aboutme .photos #photo01
  transform: rotate(-12deg);
  -webkit-transform: rotate(-12deg)
  -moz-transform: rotate(-12deg)
  -o-transform: rotate(-12deg)
  z-index 2
#aboutme .photos #photo02
  margin-top -1em
  transform: rotate(12deg)
  -webkit-transform: rotate(12deg)
  -moz-transform: rotate(12deg)
  -o-transform: rotate(12deg)

#aboutme .photos .shadow img
  display block
  margin 2%
  width 96%

#aboutme .skills
  font-family: Rokkitt, serif
  list-style-type: none
  margin-left: 2%
  padding: 0
  width: 16%

#aboutme .skills h3
  font-size: 1.8em
  font-weight: 400
  margin: 0 0 .5em
  text-shadow: 1px 2px #f9f9f9

#aboutme .skills li
  margin-bottom: .9em
  text-shadow: 1px 1px #f9f9f9

#aboutme .skills li .bar
  background-color: #7A818A
  width: 100%
  height: 6px
  border-radius: 3px

#aboutme .skills li .bar .level
  background-color: #FF9DC4
  width: 10%
  height: 100%
  border-radius: 3px



#container
  color: #464B51
  margin: 4em auto 0
  max-width: 1000px
  position: relative
  text-align: left

#container_bg_0
  background-image: url('../img/body_bg_0.jpg')
  background-position: left top
  background-repeat: repeat-x
  background-attachment: scroll
  height: 100%
  min-height: 100%

#container_bg_1
  background-image: url('../img/body_bg_1.jpg')
  background-position: center top
  background-repeat: repeat-y
  background-attachment: scroll
  height: auto !important
  min-height: 100%

#container_bg_2
  background-color: transparent
  background-image: url('../img/body_bg_2.jpg')
  background-position: center top
  background-repeat: no-repeat
  background-attachment: scroll
  overflow: hidden



#home
  box-sizing border-box
  -webtik-box-sizing border-box
  -moz-box-sizing border-box
  padding: 0 1%


#home > article
  background: #fff
  border 1px solid rgba(89,96,102, .3)
  box-sizing border-box
  -webtik-box-sizing border-box
  -moz-box-sizing border-box
  box-shadow 0 0 .2em .1em rgba(89,96,102, .15)
  display: inline-block
  margin-right 4%
  overflow: hidden
  width: 30%
#home > article:last-child
  margin-right: 0

#home > article > a
  color color1
  text-decoration none

#home > article > a:hover
  color color2

#home > article .info h2
  font-family Cookie, cursive
  font-size 2.5em
  font-weight: 400
  margin .3em 0
  padding 0
  text-align center
  -webkit-text-stroke 0.3px #666

#home > article .info p
  display none

#home > article .shadow
  position relative
  margin: 4%
  width: 92%

#home > article .shadow::before
  display: block
  content: ''
  position: absolute
  height 100%
  box-shadow: 0 0 .2em .1em rgba(89,96,102, .3) inset
  width: 100%

#home > article .shadow > img
  display: block
  width: 100%


strong
  font-weight normal

.firstcolor
  color color1

.secondcolor
  color color2

.block
  display: block

.center
  text-align: center

.bold
  font-weight: bold

.clear
  clear: both

.hidden
  overflow: hidden

.relative
  position: relative

.floatl
  float: left

.floatr
  float: right

.marginR0
  margin-right: 0 !important


/* MEDIA 800px */
@media (max-width: 800px)
  footer #contact
    background-size: 800px

  footer #contact .email, footer #contact .email:hover
    background-size: 120px
    height: 60px
    width: 60px
  footer #contact .email
    background-position 0px 0px
  footer #contact .email:hover
    background-position -60px 0px


  footer #contact .linkedin, footer #contact .linkedin:hover
    background-size: 114px
    height: 51px
    width: 51px
  footer #contact .linkedin
    background-position 0px -58px
  footer #contact .linkedin:hover
    background-position -51px -58px

  footer #contact .twitter, footer #contact .twitter:hover
    background-size: 114px
    height: 51px
    width: 51px
  footer #contact .twitter
    background-position 0px -109px
  footer #contact .twitter:hover
    background-position -51px -109px

  #aboutme .info
    margin-left 6%
    width 50%
  #aboutme .info p
    font-size 1em

  #aboutme .photos
    margin-left 2%
    width 40%

  #aboutme .skills
    margin 5% 0 0 1%
    display block
    width 100%
  #aboutme .skills h3
    margin-bottom .7em
  #aboutme .skills > li
    margin 0 4% 3% 0
    padding 0
    display inline-block
    width 16%
  #aboutme .skills > li:nth-child(5n+1)
    margin-right 0

  
  #home > article .info h2
    font-size 2.2em



/* MEDIA 640px */
@media (max-width: 640px)
  footer #contact
    background-size: 700px

  footer #contact h2
    font-size 2.5em

  header h1
    font-size 3.6em

  header h2
    font-size 1.5em

  #aboutme .info, #aboutme .photos
    display block
    width 100%

  #aboutme .info
    width 90%

  #aboutme .info h2
    font-size 2.8em
    margin-bottom .3em

  #aboutme .info .textinfo
    font-size 1.2em
  #aboutme .info p
    margin-bottom .8em

  #aboutme .photos
    margin 9% 0 7%
    padding 0 7%
  
  #aboutme .photos .shadow::before  
    height 94%

  #aboutme .photos #photo01
    display none
  #aboutme .photos #photo02
    transform: rotate(0deg)
    -webkit-transform: rotate(0deg)
    -moz-transform: rotate(0deg)
    -o-transform: rotate(0deg)

  #aboutme .skills
    margin 10% 0 0 0
    padding 0 6%
  #aboutme .skills h3
    font-size 2em
    margin-bottom .6em
  #aboutme .skills > li
    width 47%
    margin-right 5%
  
  #aboutme .skills > li:nth-child(6)
    margin-right 5% !important
  #aboutme .skills > li:nth-child(odd)
    margin-right 0


  #home > article .info h2
    font-size 1.9em



/* MEDIA 480px */
@media (max-width: 480px)
  footer 
    margin-bottom 1%
  
  footer #contact
    padding-bottom 8%

  header
    margin-bottom 6%
  
  header h1
    font-size 3.3em
  header h2
    font-size 1.3em

  nav ul > li
    font-size 1em
    margin 0 .8em
  
  #aboutme
    margin 12% 0 8%

  #aboutme .info h2
    margin-bottom .1em

  #aboutme .info .textinfo
    font-size 1.1em

  #aboutme .photos
    margin-bottom 8%

  #aboutme .skills > li
    width 46%


  #container
    margin-top 3.2em

  #home
    padding 0 2%

  #home > article
    display block
    margin 0 0 3% 0
    width 100%

  #home > article .shadow, #home > article .info
    box-sizing border-box
    display inline-block
    vertical-align top
    width 45%

  #home > article .info h2
    font-size 2.8em
    text-align left

  #home > article .info p
    display block
    margin 0
    padding-right 2%



/* MEDIA 360px */
@media (max-width: 360px)
  footer #contact h2
    font-size 2.3em
    padding 5% 0 1%

  footer #contact .social a
    margin-right 5%

  header h1
    font-size 3em
    padding-bottom: .1em

  header h2
    font-size 1.1em

  #aboutme .photos
    margin-bottom 8%

  #home > article .info h2
    margin 10% 0 2% 2%
    font-size 2.4em